<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Remainder"
             xmlns:converters="clr-namespace:Remainder.Converters"
             x:Class="Remainder.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
                
            </ResourceDictionary.MergedDictionaries>
            <converters:CompleteIconConverter x:Key="CompleteIconConverter" />
            <converters:CompleteColorConverter x:Key="CompleteColorConverter" />
        </ResourceDictionary>
        

    </Application.Resources>
</Application>
